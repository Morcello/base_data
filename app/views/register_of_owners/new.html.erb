<h2>РЕЕСТР СОБСТВЕННИКОВ</h2>

<%= form_with model: @register_of_owner do |f| %>
  <div class="field">
    <%= f.label "Имя" %><br />
    <%= f.text_field :first_name, autocomplete: "first_name" %>
  </div>

  <div class="field">
    <%= f.label "Фамилия" %><br />
    <%= f.text_field :last_name, autocomplete: "last_name" %>
  </div>

  <div class="field">
    <%= f.label "Отчество" %><br />
    <%= f.text_field :middle_name, autocomplete: "middle_name" %>
  </div>

  <div class="field">
    <%= f.label "Лицевой счет" %><br />
    <%= f.text_field :personal_account, autocomplete: "personal_account" %>
  </div>

  <div class="field">
    <%= f.label "Город" %><br />
    <%= f.text_field :city, autocomplete: "city" %>
  </div>

  <div class="field">
    <%= f.label "Улица" %><br />
    <%= f.text_field :street, autocomplete: "street" %>
  </div>

  <div class="field">
    <%= f.label "Дом №" %><br />
    <%= f.text_field :house_no, autocomplete: "house_no" %>
  </div>

  <div class="field">
    <%= f.label "Квартира №" %><br />
    <%= f.text_field :apartment_no, autocomplete: "apartment_no" %>
  </div>

  <div class="field">
    <%= f.label "Кол-во собственников" %><br />
    <%= f.text_field :number_owners, autocomplete: "number_owners" %>
  </div>

  <div class="field">
    <%= f.label "Телефон" %><br />
    <%= f.text_field :phone, autocomplete: "phone" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label "Дата активации дома" %><br />
    <%= f.text_field :home_activation_date, autocomplete: "home_activation_date" %>
  </div>

  <div class="field">
    <%= f.label "Дата блокировки абонента" %><br />
    <%= f.text_field :subscriber_blocking_date, autocomplete: "subscriber_blocking_date" %>
  </div>

  <div class="field">
    <%= f.label "Серийный №" %><br />
    <%= f.text_field :serial_number, autocomplete: "serial_number" %>
  </div>

  <div class="field">
    <%= f.label "Подрядчик" %><br />
    <%= f.text_field :contractor, autocomplete: "contractor" %>
  </div>

  <div class="actions">
    <%= f.submit "add" %>
  </div>
<% end %>
